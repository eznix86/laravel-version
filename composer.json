{
  "name": "eznix86/laravel-version",
  "description": "Semantic app versioning for Laravel with git integration.",
  "type": "library",
  "require": {
    "illuminate/support": "^11.0|^12.0",
    "phlak/semver": "^6.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Eznix86\\Version\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Bruno Bernard",
      "email": "contact.brunobernard@gmail.com"
    }
  ],
  "require-dev": {
    "laravel/pint": "^1.26",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "pestphp/pest": "^4.1",
    "pestphp/pest-plugin-type-coverage": "^4.0",
    "rector/rector": "^2.2",
    "phpstan/phpstan": "^2.1",
    "larastan/larastan": "^3.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Eznix86\\Version\\VersionServiceProvider"
      ],
      "aliases": {
        "Version": "Eznix86\\Version\\Facades\\Version"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
